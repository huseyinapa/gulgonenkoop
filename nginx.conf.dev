events {
    worker_connections 1024;
}

http {
    server {
        listen 80;
        server_name gulgonenkoop.com www.gulgonenkoop.com;

        location / {
            proxy_pass http://18.118.53.1:3202;
        }
    }
    server {
       listen 443 ssl;
       server_name gulgonenkoop.com  www.gulgonenkoop.com;

       ssl_certificate /etc/nginx/ssl/gulgonen/server.crt;
       ssl_certificate_key /etc/nginx/ssl/gulgonen/server.key;

       ssl_protocols TLSv1.2 TLSv1.3;
       ssl_ciphers HIGH:!aNULL:!MD5;

       location / {
           proxy_pass http://18.118.53.1:3202;  # Docker'daki backend servisine y√∂nlendir
       }
}


    server {
        listen 80;
        server_name huseyinapa.com www.huseyinapa.com;

        location / {
            proxy_pass http://18.118.53.1:3200;
        }
    }
}
